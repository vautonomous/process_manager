<launch>

    <group>
        <push-ros-namespace namespace="ui"/>
        <node pkg="ui_process_manager" exec="ui_process_manager_prime_node" output="screen">
            <remap from="in/process_command" to="~/ui_process_command"/>
            <remap from="in/process_result" to="/ui/ui_process_manager_second_node/ui_process_diagnostic"/>
            <remap from="out/process_result" to="~/ui_process_diagnostic"/>
            <remap from="out/process_command" to="/ui/ui_process_manager_second_node/ui_process_command"/>
            <param from="$(find-pkg-share ui_process_manager)/config/ui_process_manager.param.yaml"/>
        </node>
    </group>

</launch>